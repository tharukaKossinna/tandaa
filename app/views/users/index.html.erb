<h1> ADNAT </h1>

<%= form_for :login, url: users_path do %>
    <p> <%= label_tag(:q, "Email") %> </p>
    <p> <%= text_field_tag(:q) %> </p>
    <p> <%= label_tag(:p, "Password") %> </p>
    <p> <%= text_field_tag(:p) %> </p>
    
    <%= submit_tag 'Login' %>
    <%= link_to "Sign Up", new_user_url %>
<% end %>